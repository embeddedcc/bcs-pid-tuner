body
  font: 14px "Lucida Grande", Helvetica, Arial, sans-serif
a
  color: #00B7FF

i.checkmark
  margin-top: -1.3em
  font-size: 1.5em
  margin-right: 0.2em

div.has-success i
  color: green

div.has-error i
  color: red

.row
  height: 1.8em

.header 
  height 2.5em
  
  & div
    text-decoration underline
    font-weight bold

.tuning
  margin-top 45px
  margin-left -15px
  padding-right 30px
  border 1px solid black
  width 50%

  & .row
    margin-left 0px
    text-align center
    padding-right 10px
    
  & .list
    overflow-y scroll
    overflow-x hidden
    height 300px
  & .col-md-2
    width 20%
    
input.form-control
  padding-right 6px

.col-md-1
  padding-left 0
  
.heading
  position relative
  top -1.8em
  left 0px
  text-decoration underline
  font-weight bold